{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "es2015", "es2017"],
    "module": "commonjs",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6",
    "types": ["@emotion/react/types/css-prop", "node"]
  },
  "include": ["./src/**/*"]
}
